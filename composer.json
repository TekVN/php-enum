{
    "name": "dnt/php-enum",
    "description": "Enum utility for you.",
    "keywords": [
        "php",
        "enum",
        "DNT",
        "ducconit"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DNT\\Enum\\": "src/"
        }
    },
    "authors": [
        {
            "name": "DNT",
            "email": "ducconit@gmail.com",
            "homepage": "https://github.com/ducconit"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.0|^3.0",
        "phpstan/phpstan": "^1.0|^2.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse"
    }
}
